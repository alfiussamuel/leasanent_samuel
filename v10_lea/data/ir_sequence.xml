<odoo>
	<data>				

		<!-- Replenishment -->
		<record id="sequence_lea_replenishment" model="ir.sequence">  
          <field name="name">LEA Replenishment</field>  
          <field name="code">lea.replenishment</field>  
          <field name="prefix">REP/%(y)s/%(month)s/</field>  
          <field name="padding">5</field>  
          <field eval="1" name="number_increment"/>  
        </record>
        		
		<!-- Petty Cash -->
		<record id="sequence_lea_petty_cash" model="ir.sequence">  
          <field name="name">LEA Petty Cash</field>  
          <field name="code">lea.petty.cash</field>  
