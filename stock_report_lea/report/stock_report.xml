                    <field name="stock_retur" type="measure"/>
                    <field name="stock_sale" type="measure"/>
                    <field name="stock_akhir" type="measure"/>
                    <field name="stock_gudang" type="measure" disable_linking="True"/>
	            </pivot>
	        </field>
	    </record>


